<p id="notice"><%= notice %></p>

<h1>Listing Order Item Statuses</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @order_item_statuses.each do |order_item_status| %>
      <tr>
        <td><%= order_item_status.name %></td>
        <td><%= link_to 'Show', order_item_status %></td>
        <td><%= link_to 'Edit', edit_order_item_status_path(order_item_status) %></td>
        <td><%= link_to 'Destroy', order_item_status, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Order item status', new_order_item_status_path %>
